<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="Grid" parent="GtkWidget">
        <child>
            <object class="GridBuffer" id="buffer">
                <property
                    name="font"
                    bind-source="Grid"
                    bind-property="font"
                    bind-flags="sync-create"
                />
                <property
                    name="scroll-delta"
                    bind-source="Grid"
                    bind-property="viewport-delta"
                />
                <property
                    name="scroll-transition"
                    bind-source="Grid"
                    bind-property="scroll-transition"
                    bind-flags="sync-create"
                />
            </object>
        </child>
        <child>
            <object class="Cursor" id="cursor">
                <property
                    name="font"
                    bind-source="Grid"
                    bind-property="font"
                    bind-flags="sync-create"
                />
                <property
                    name="busy"
                    bind-source="Grid"
                    bind-property="busy"
                    bind-flags="sync-create"
                />
                <property
                    name="active"
                    bind-source="Grid"
                    bind-property="active"
                    bind-flags="sync-create"
                />
                <property
                    name="mode-info"
                    bind-source="Grid"
                    bind-property="mode-info"
                    bind-flags="sync-create"
                />
                <property
                    name="blink-transition"
                    bind-source="Grid"
                    bind-property="cursor-blink-transition"
                    bind-flags="sync-create"
                />
                <property
                    name="position-transition"
                    bind-source="Grid"
                    bind-property="cursor-position-transition"
                    bind-flags="sync-create"
                />
            </object>
        </child>
    </template>
</interface>


